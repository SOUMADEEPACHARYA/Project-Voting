        {
            
            //Fill the code here
            StreamReader sr = new StreamReader("input.csv");
                string line;
                while((line = sr.ReadLine()) != null)
                {
                    //string[] details = line.Split(new char[]{','});
                    //Product p=new Product(details[0],details[1],Convert.ToDateTime(details[2]),Convert.ToDouble(details[3]));
                    Console.WriteLine(line);
                }
        }
    }
    
    
    
    
    using System;
using System.Collections.Generic;
using System.Text;

    public class Product
    {
        private string _productName;
        private string _serialNumber;
        private DateTime _purchaseDate;
        private double _cost;
        
        // Implement 4-Argument Constructor
        
        // Implement Properties
       
        public override string ToString()
        {
            return String.Format("{0,-15}{1,-15}{2,-15}{3,-15}", _productName, _serialNumber, String.Format("{0:d}", _purchaseDate), _cost);
        }
    }





